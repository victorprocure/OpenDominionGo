package home

import "github.com/victorprocure/opendominiongo/cmd/web"

templ Home() {
    @web.Page(web.PageOpts{ShowNav: true}) {
        <div class="flex justify-center my-8">
            <img class="select-none cursor-pointer ease-in-out duration-300 transition-all lg:w-[220px] w-30"
                 src="/assets/opendominion.png" alt="OpenDominion" title="Where power still prevails" />
        </div>
    }
}